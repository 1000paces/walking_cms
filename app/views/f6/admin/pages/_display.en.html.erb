<%= render :partial => "admin/pages/header", :locals => {:page => @page} -%>
<div class="<%= @page.setting.container_type -%> wcms-master-row">
	<%= render :partial => "admin/pages/nav_vertical", :locals => {:side => 'left'} if @page.setting.nav_location == 4 -%>
	<div class="medium-<%= [4,5].include?(@page.setting.nav_location) ? '10' : '12' %> columns" id="wcms-work-area">
		<div class="<%= @page.setting.container_type -%>" id="wcms-navigation-row">
			<div class="medium-12 navigation-zone" id="wcms-navigation-row-inner">
				<%= render :partial => "admin/pages/nav_horizontal" if @page.setting.nav_location < 3 && !@page.setting.overlap -%>
			</div>
		</div>
		<%- @page.rows.each_with_index do |row, index| -%>
			<%- unless row.new_record? -%>
				<%= render :partial => "admin/pages/row", :locals => {:index => index, :row => row} -%>
			<%- end -%>
		<%- end -%>
		<div class="row">
			<div class="medium-12 footer-zone zone" id="footer-row">
				<%= render :partial => "admin/pages/nav_horizontal" if @page.setting.nav_location == 3 -%>
			</div>
		</div>
	</div>
	<%= render :partial => "admin/pages/nav_vertical", :locals => {:side => 'right'} if @page.setting.nav_location == 5 -%>
</div>