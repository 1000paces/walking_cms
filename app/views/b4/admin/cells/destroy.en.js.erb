$("#wcms-cell-<%= @cell.id %>").fadeOut();
$("#wcms-undo").parent().removeClass("hidden");
$("#wcms-restore-page-id").val("<%= @page.id -%>");
$("#wcms-restore-row-id").val("<%= @row.id -%>");
$("#wcms-restore-cell-id").val("<%= @cell.id -%>");
<%- @dead_rows.each do |dr| -%>
	$("#wcms-row-<%= dr %>").remove();
<%- end -%>
$("#wcms-page-undo").delay(2000).effect("highlight", {}, 3000);

